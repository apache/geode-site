T{;{;U;[I"</geode-site/website/content/css/bootflat-extensions.css;
TI"ö#/*
    Bootflat 1.0.1
    Designed & Built by flathemes, http://www.flathemes.com
    Licensed under MIT License, http://opensource.org/licenses/mit-license.html

    Thanks for supporting our website and enjoy!
*/
/*------------------------------------*\
    $bubble
\*------------------------------------*/
.bubble-body {
    position: relative;
    padding: 3px;
    background-color: #ecf0f1;
    border-radius: 3px;
    overflow: visible;
}
.pull-left ~ .bubble-body {
    margin-left: 60px;
}
.pull-right ~ .bubble-body {
    margin-right: 60px;
}
.bubble-body .bubble-inner {
    min-height: 32px;
    border: 1px solid #d3d7d7;
    background-color: #fbfbfb;
}
.bubble-body .bubble-heading {
    padding: 0 10px;
    border-bottom: 1px solid #d3d7d7;
    background-color: #f4f4f4;
    font-size: 12px;
    font-weight: bold;
    color: #222;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 33px;
    line-height: 33px;
}
.bubble-body .bubble-content {
